{
  "name": "vscode-haml-formatter",
  "displayName": "Haml Formatter",
  "description": "Adds a haml formatter to vscode",
  "version": "0.0.1",
  "icon": "resources/images/icon.png",
  "publisher": "Syler",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheRealSyler/vscode-haml-formatter"
  },
  "engines": {
    "vscode": "^1.42.0"
  },
  "activationEvents": [
    "onLanguage:haml"
  ],
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "vscode:prepublish": "npm run build && suf",
    "build": "webpack --mode production",
    "watch": "webpack --mode development"
  },
  "main": "./dist/extension.js",
  "devDependencies": {
    "@types/node": "^12.12.29",
    "@types/vscode": "^1.42.0",
    "suf-cli": "^0.1.0",
    "ts-loader": "^6.2.1",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "haml-formatter": "^0.1.0"
  }
}
